<div>
  <div class="row pb-5 align-items-center">
    <div class="col-4 d-flex justify-content-start align-items-center">
      <%= link_to admin_places_path, class: 'btn btn-outline-secondary border-0 ps-2' do %>
        <i class="fas fa-arrow-left"></i>
        <%= t('common.back_to_list') %>
      <% end %>
     <h5 class="mb-0"><%= t('places.title.deleted')%></h5>
    </div>
    <div class='col-8 pe-4'>
      <%= render 'admin/places/search_form'%>
    </div>
  </div>
  
  <%= render 'table_places', places: @places, q: @q %>
  <%= render 'layouts/admin/pagination', paging_collection: @suppliers %>
</div>
